/* pages/details/setting/setting.wxss */
.box{
  width: 92%;
  margin: 0 auto;
}
.list{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  padding: 0 32rpx;
  box-sizing: border-box;
  box-shadow:0px 3px 6px rgba(0,0,0,0.16);
  border-radius: 4rpx;
  display: flex;
  justify-content: space-between;
  margin-top:32rpx;
  /* align-items: center; */
}
.list .left image{
  width: 40rpx;
  vertical-align: middle;
}
.list .left text{
  font-size:28rpx;
  color:rgba(35,35,35,1);
  margin-left: 20rpx;
}
.point{
  width: 16rpx;
  margin-top: 24rpx;
}
.sub{
  width: 100%;
  height:80rpx;
  line-height: 56rpx;
  border-radius:40rpx;
  color: #fff;
  font-weight: 400;
  font-size:28rpx;
  margin-top: 20%;
}