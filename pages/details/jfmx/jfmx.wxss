/* pages/details/jfmx/jfmx.wxss */
.list_box{
  width: 91%;
  margin: 0 auto;
  margin-top: 50rpx;
}
.list{
  width:100%;
  height:160rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 3px 10px rgba(0,0,0,0.16);
  border-radius:10px;
  display: flex;
  justify-content: space-between;
  margin-bottom: 32rpx;
}
.right{
  width:108rpx;
  height:160rpx;
  background:rgba(255,63,49,1);
  border-radius:0px 20rpx 20rpx 0px;
  color: #fff;
  writing-mode: vertical-lr;
  text-align: center;
  letter-spacing: 20rpx;
  padding-left: 28rpx;
  box-sizing: border-box;
  padding-top: 16rpx;
}
.left{
  line-height: 45rpx;
  padding: 17rpx 0 0 30rpx;
  box-sizing: border-box;
}
.left text{
  display: block;
  color: #909090;
  font-size: 24rpx;
}
.left text.mon{
  font-size:32rpx;
  font-weight:bold;
  color: #E10004;
}
.tk{
  background: #FCA800;
}
.xf{
   background: #08D289;
}