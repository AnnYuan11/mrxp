  top: 28%;
  height: 100rpx;
  left: 28rpx;
}
.orderBuy{
  height: 60rpx;
  line-height: 60rpx;
  background: rgba(0,0,0,0.4);
  border-radius: 30rpx;
  padding: 0 30rpx 0 0;
  display: flex;
}
.orderBuy text{
  font-size: 24rpx;
  color: #fff;
