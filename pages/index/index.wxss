.flow .sure{
  width:340rpx;
  height:90rpx;
  line-height: 90rpx;
  background:linear-gradient(90deg,rgba(255,232,119,1) 0%,rgba(254,225,47,1) 100%);
  box-shadow:0px 4px 10px rgba(0,0,0,0.16);
  border-radius:46rpx;
  text-align: center;
  font-size:32rpx;
  color:rgba(0,0,0,1);
