/* pages/index/index.wxss */
/* 头部 */
.headBox{
  width: 100%;
  height:220rpx;
  background:linear-gradient(180deg,rgba(225,0,4,1) 0%,rgba(228,78,41,1) 100%);
  padding: 32rpx;
  box-sizing: border-box;
}
.headBox .title{
  font-size:36rpx;
  color:rgba(255,255,255,1);
  margin-top: 30rpx;
}
.weather{
  margin-top: 48rpx;
  display: flex;
  color: #fff;
  justify-content: space-between;
}
.boxB{
  overflow: scroll;
}
.box{
  width: 100%;
  background: #fff;
  padding: 32rpx;
  box-sizing: border-box;
}
.date{
  display: flex;
  align-content: center;
}
/* 搜索 */
.search_Box{
  width:100%;
  height: 75rpx;
  display: flex;
  margin: 20rpx 0;
  justify-content:space-around; 
}
.search_left{
  width:100%;
  background:rgba(243,243,243,1);
  border-radius:32px;
  padding: 18rpx 32rpx;
  box-sizing: border-box;
  display: flex;
}
.search_left image{
  width: 34rpx;
  margin-top: 6rpx;
}
.serch{
  font-size:24rpx;
  color:rgba(194,194,194,1);
}
.search_left input{
  margin-left: 20rpx;
}
.search_right{
  display: flex;
  align-items: center;
  background: red;
  justify-content: space-between;
  border-radius: 50rpx;
  padding: 4rpx 20rpx;
  box-sizing: border-box;
  margin-left: 40rpx;
}
.search_right image{
  width:48rpx;
}
.search_right text{
  color: #fff;
  width: 48%;
  font-size: 24rpx;
  display: inline-block;
}
/* 顶部 */
.top{
  display: flex;
  justify-content: space-between;
}
.img image{
  width: 108rpx;
  margin-top: 5%；
}
.message{
  width: 80%;
  line-height: 47rpx;
  margin-left: 40rpx;
}
.message .name{
  font-size:30rpx;
  font-weight:bold;
  color:rgba(35,35,35,1);
}

.qhdz_box text{
  font-size: 24rpx;
  color: rgba(35,35,35,1);
}
.qhdz_box .address{
  color: #909090;
}
.line{
  width: 2px;
  height: 92rpx;
  background: #E2E2E2;
}
.fans{
  width: 200rpx;
  text-align: center;
  line-height: 50rpx;
}
.fans text{
  display: block;
  font-size: 24rpx;
  color: #909090;
}
.fans text.num{
  font-size: 30rpx;
  color: #F66228;
}
/* 轮播 */
swiper{
  width: 100%;
  height: 280rpx;
}
swiper image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
  width:40rpx;
  display: inline-flex;
  height: 12rpx;
  margin-left: 10rpx;
}
.swiper-box .wx-swiper-dot::before{
  content: '';
  flex-grow: 1; 
  background: #fff;
  border-radius: 8rpx;
  width:40rpx;
  display: inline-flex;
  height: 12rpx;
}
.swiper-box .wx-swiper-dot-active{
  width:12rpx;
  display: inline-flex;
  height: 10rpx; 
}
/* 广播 */
.notice{
  width: 100%;
  height:83rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
}
.swiper_item{
  font-size:24rpx;
  color:#909090;
}
.swiper_container{
  height:40rpx;
  font-size:24rpx;
}
/* 商品 */
.details_box{
  /* background: #fff; */
  width: 100%;
  /* padding: 0 32rpx;
  box-sizing: border-box; */
  padding-bottom: 100rpx;
}
.swiper-tab {
  width: 100%;
  line-height: 80rpx;
  background: #fff;
}

.swiper-tab-list {
  font-size: 32rpx;
  display: inline-block;
  width: 50%;
  color: #9b9b9b;
  text-align: center;
}

.on {
  color: #232323;
  font-weight:bold;
  height: 70rpx; 
  border-bottom: 8rpx solid#FF5041;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.swiperBox{
  margin:32rpx auto;
  width: 96%;
 
}
.img_goods{
  width: 100%;
  position: relative;
}
.img_goods image{
  width: 100%;
  height: 420rpx;
  border-radius: 8rpx;
}
.img_goods image.zt{
  position: absolute;
  right: 3%;
}
.supernatant{
  background: rgba(0,0,0,0.6);
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 999999;
}
.type{
  width:150rpx;
  height:40rpx;
  line-height: 40rpx;
  background:rgba(255,217,54,1);
  opacity:0.9;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  position: absolute;
  left: 0;
  top:0;
  z-index: 99999;
  border-radius:4px 0px 13px 0px;
}
.type2{
  width:150rpx;
  height:40rpx;
  line-height: 40rpx;
  background:#08D288;
  opacity:0.9;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  position: absolute;
  left: 0;
  top:0;
  z-index: 99999;
  border-radius:4px 0px 13px 0px;
}
.subtitle{
  display: inline-block;
  max-width: 400rpx;
  border-radius:0px 20px 20px 4px;
  /* height: 40rpx; */
  /* line-height: 37rpx; */
  background: rgba(0,0,0,0.5);
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  padding:16rpx 20rpx;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  bottom: 8rpx;

}
.bottom_box{
  width: 98%;
  margin:0 auto;
}
.bottom_box .spmc{
  width: 100%;
  font-size:32rpx;
  color:#000;
  font-weight: 600;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
 
}
.thsj{
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}
.thsj .left{
  /* display: flex;
  justify-content: space-between; */
}
.thsj .left text{
  display:block;
  height:50rpx;
  line-height: 50rpx;
  font-size:24rpx;
  color:#000;
  text-align: center;
  /* margin-bottom: 10rpx; */
}

.thsj .left text.rq{
  color: green;
}
.thsj .right text{
  font-size:28rpx;
  color:rgba(35,35,35,1);
}
.thsj .right text.weight{
  font-size:30rpx;
  font-weight: 600;
}
.thsj .right text.num{
  color: green;
  font-size: 32rpx;
  font-weight: 600;
}
.thsj .right .limited{
  color: green;
  display: block;
}
.price_box{
  display: flex;
  justify-content: space-between;
  margin-top: 32rpx;
  padding-bottom: 32rpx;
  border-bottom: 2rpx solid #E2E2E2;
}
.price text{
  font-size:56rpx;
  font-weight:bold;
  color:rgba(231,0,18,1);
}
.jrgec{
  height: 80rpx;
  line-height: 80rpx;
  background: rgba(225,0,4,1);
  border-radius: 30px;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  padding: 0 50rpx;
}
.over{
  background: #CECECE;
  color: #727272;
}
.list_box{
  background: #fff;
  padding: 10rpx;
  box-sizing: border-box;
  margin-bottom: 32rpx;
}

/* 视频 */
video {
  width: 100%;
  height: 400rpx;
}
/* 弹窗 */
.flow{
  width: 100%;
  height: 100vh;
  background:rgba(0,0,0,0.6);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99999999;
}
.img_box{
  width: 80%;
  margin:0 auto;
  position: relative;
}
.img_box image{
  width: 100%;
}
.text{
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  top: 25%;
}
.text text{
  color: #fff;
  font-size: 48rpx;
  display: block;
}
.text text.preferential{
  font-size: 32rpx;
}
.yhq_box{
  width: 96%;
  margin:0 auto;
  position: absolute;
  left: 0;
  right: 0;
  top: 36%;
}
.yhq{
  width: 100%;
  position: relative;
  margin-top: -6%;
}
.yhq image{
  width: 100%;
}
.yhq .left{
  position: absolute;
  top: 36%;
  left: 12%;
}
.yhq .left text{
  font-size:24rpx;
  color:rgba(248,58,54,1);
}
.yhq .left text.num{
  font-size: 48rpx;
}
.yhq .right{
  position: absolute;
  top: 25%;
  right: 20%;
  line-height: 48rpx;
}
.yhq .right text{
  display: block;
  font-size: 24rpx;
  color: #919191;
}
.yhq .right text.yhqs{
  font-size: 32rpx;
  color: #000000;
}
