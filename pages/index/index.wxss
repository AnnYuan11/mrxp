.close{
  text-align: center;
}
/* 粉丝关注 */
.fsgz{
  width: 35%;
  text-align: center;
  border-left: 2px solid #E2E2E2;

}
.thsj .fsgz .limited{
  font-size: 30rpx;
  font-weight: 600;
}
/* 购买记录 */
.buy_box{
  width: 100%;
  display: flex;
  align-items: center;
}
.box_figure{
  max-width: 73%;
  display: flex;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;

}

.figure{
  width: 150rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.figure image{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  vertical-align: middle
}
.figure text{
  font-size: 20rpx;
  margin-left: 8rpx;
}
.so{
  font-size:24rpx;
  color:rgba(144,144,144,1);
  margin-top: 8rpx;
}
/* 返回顶部 */
.back{
  color: #fff;
  text-align: center;
  position: fixed;
  bottom: -18rpx;
  right: 0rpx;
}

.back image{
  width: 140rpx;
}
/* 分享 */
.share{
  position: fixed;
  bottom:100rpx;
  right: 0rpx;
 }
 .share image{
  width:140rpx;
 }
 button{
  font-size: 28rpx!important;
